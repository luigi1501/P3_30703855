<body>
  <h1 class="my-title">detalles</h1>
  
  <div class="product-grid">
              <div class="product">
                <a href="/payments" class="compra">Comprar ahora</a>
                <h2><%= producto.name %></h2>
                <p>Precio: <%= producto.price %></p>
                <p>Descripci√≥n: <%= producto.description %></p>
                <p>Categoria: <%= producto.category_id %></p>
                <p>Modelo: <%= producto.brand %></p>
                <p>Marca: <%= producto.model %></p>
                <% imagen.forEach(images => { %>
                  <% if (producto.id == images.producto_id) { %>
                    <img alt="Product image" src="<%= images.url %>" width="200" height="150">
                  <% } %>
                <% }); %>
      </div>
      
</div>
